include ../p/p
include ../title/title

mixin about-block(options)
    -
        options = options || {};
        const {data = []} = options;
    section.block
        .block__title
            +title({text: "Кратко о компании"})
        .block__el.block__el_margin_normal
            for item in data
                +p({text: item.text})