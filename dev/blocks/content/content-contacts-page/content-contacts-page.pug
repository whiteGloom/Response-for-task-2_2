include ../../map/map
include ../../menagers-block/menagers-block
include ../../contact-us-block/contact-us-block
include ../../header/header

mixin content-contacts-page(options = {})
    -
        const {mapData = {}, menagersData = {}, contactUsData = {}, title = ""} = options;
    main.content-contacts-page
        .content-contacts-page__header
            +header({title})
        .content-contacts-page__body
            .content-contacts-page__section
                +map(mapData)
            .content-contacts-page__section
                +menagers-block(menagersData)
            .content-contacts-page__section
                +contact-us-block(contactUsData)