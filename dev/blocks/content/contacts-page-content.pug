include ../map/map
include ../menagers-block/menagers-block
include ../contact-us-block/contact-us-block

mixin contacts-page-content(options)
    -
        options = options || {};
        const {mapData = {}, menagersData = {}, contactUsData = {}} = options;
    section.base__section
        +map(mapData)
    section.base__section
        +menagers-block(menagersData)
    section.base__section
        +contact-us-block(contactUsData)